# Azure OpenAI 설정
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name

# 네이버 검색 API 설정
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# Teams 앱 설정
TEAMS_APP_ID=your_teams_app_id
TEAMS_APP_PASSWORD=your_teams_app_password
TEAMS_APP_TENANT_ID=your_tenant_id

# Microsoft Graph API 설정
MICROSOFT_GRAPH_CLIENT_ID=your_graph_client_id
MICROSOFT_GRAPH_CLIENT_SECRET=your_graph_client_secret
MICROSOFT_GRAPH_TENANT_ID=your_tenant_id

# 서버 설정
HOST=0.0.0.0
PORT=8000
DEBUG=true
